{
    "manifest_version" : 3 ,
    "name": "Tweet Summarization",
    "version" : "1.0",
    "permissions": ["activeTab"],
    "host_permissions": [
        "http://localhost:5000/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup":"popup.html"
    }
}